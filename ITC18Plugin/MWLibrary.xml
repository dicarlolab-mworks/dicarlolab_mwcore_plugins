<MWElements>
	<MWElement name="ITC-18 IO Device">
		<!-- XPath 2.0 expression defining nodes of this type -->
		<match_signature>iodevice[@type="itc18"]</match_signature>
		
		<!-- Inheritance -->
		<isa>IODevice</isa>
		
		<description>
Used for interfacing to an ITC-18
		</description>
		<icon>smallIOFolder</icon>
		
		<!-- Payload -->
		<code>
			<iodevice tag="ITC-18 IO Device" priority="" type="itc18" alt=""/>
			</code>
		</MWElement>	
	
	<MWElement name="ITC-18 Analog Input Channel">
		<!-- XPath 2.0 expression defining nodes of this type -->
		<match_signature>iochannel[@type="itc18_analog_input"]</match_signature>
		
		<!-- Inheritance -->
		<isa>IOChannel</isa>
		<allowed_parent>ITC-18 IO Device</allowed_parent>
		
		<description>
Used for interfacing to an analog channel on an ITC-18 
		</description>
		<icon>smallIOFolder</icon>
		
		<!-- Payload -->
		<code>
			<iochannel tag="ITC analog input" variable="" capability="ITC18_ADC_INPUT0" data_type="analog" type="itc18_analog_input" direction="input" synchrony="hardware_timed_synchronous" range_min="-10" range_max="10" resolution="16" update_interval="3ms" data_interval="1ms"/>
			</code>
		</MWElement>	
	
	<MWElement name="ITC-18 TTL Input Channel">
		<!-- XPath 2.0 expression defining nodes of this type -->
		<match_signature>iochannel[@type="itc18_ttl_digital_input"]</match_signature>
		
		<!-- Inheritance -->
		<isa>IOChannel</isa>
		<allowed_parent>ITC-18 IO Device</allowed_parent>
		
		<description>
Used for interfacing to a TTL input channel on an ITC-18.  Possible values for data_type are edge_low_to_high, edge_high_to_low, edge_any, or digital for continuous digital data.
		</description>
		<icon>smallIOFolder</icon>
		
		<!-- Payload -->
		<code>
			<iochannel tag="ITC-18 TTL input" type="itc18_ttl_digital_input" variable="" capability="ITC18_TTL_INPUT0" data_type="edge_low_to_high" direction="input" synchrony="hardware_timed_synchronous" range_min="0" range_max="0" resolution="1" update_interval="3ms" data_interval="0.5ms"/>
			</code>
		</MWElement>	
	
	<MWElement name="ITC-18 AUX TTL Output Channel">
		<!-- XPath 2.0 expression defining nodes of this type -->
		<match_signature>iochannel[@type="itc18_async_ttl_digital_output"]</match_signature>
		
		<!-- Inheritance -->
		<isa>IOChannel</isa>
		<allowed_parent>ITC-18 IO Device</allowed_parent>
		
		<description>
Used for interfacing to an asynchronous auxiliary TTL output channel on an ITC-18.  Possible values for data_type are digital, pulse_active_high, pulse_active_low, or digital_uint8.
		</description>
		<icon>smallIOFolder</icon>
		
		<!-- Payload -->
		<code>
			<iochannel tag="ITC-18 aux output" type="itc18_async_ttl_digital_output" variable="" capability="ITC18_TTL_ASYCH_OUT00" data_type="digital" direction="output" synchrony="asynchronous" range_min="0" range_max="0" resolution="1" update_interval="3ms" data_interval="1ms"/>
			</code>
		</MWElement>	
	
	<MWElement name="ITC-18 Triggered Analog Snippet">
		<!-- XPath 2.0 expression defining nodes of this type -->
		<match_signature>iochannel[@type="itc18_triggered_analog_snippet"]</match_signature>
		
		<!-- Inheritance -->
		<isa>IOChannel</isa>
		<allowed_parent>ITC-18 IO Device</allowed_parent>
		
		<description>
Used for recording an analog snippet on an ITC-18.  A TTL channel is used to trigger the snippet.
		</description>
		<icon>smallIOFolder</icon>
		
		<!-- Payload -->
		<code>
			<iochannel tag="ITC-18 Triggered Analog Snippet" type="itc18_triggered_analog_snippet" variable="" capability="ITC18_ADC_INPUT0" data_type="triggered_analog_snippet" direction="output" synchrony="hardware_timed_synchronous" range_min="-10" range_max="10" resolution="16" update_interval="3ms" data_interval="70us" TTL_trigger_port="0" pre_trigger_interval="4ms" post_trigger_interval="4ms"/>
			</code>
		</MWElement>	
	
	<MWElement name="ITC-18 Word Output Channel">
		<!-- XPath 2.0 expression defining nodes of this type -->
		<match_signature>iochannel[@type="itc18_word_output"]</match_signature>
		
		<!-- Inheritance -->
		<isa>IOChannel</isa>
		<allowed_parent>ITC-18 IO Device</allowed_parent>
		
		<description>
Used for sending word output on an ITC-18 (e.g. to a Plexon system for clock synchronization)
		</description>
		<icon>smallIOFolder</icon>
		
		<!-- Payload -->
		<code>
			<iochannel tag="ITC-18 word output" type="itc18_word_output" variable="" capability="ITC18_TTL_AUX_WORD1" data_type="digital_uint8" direction="output" synchrony="asynchronous" range_min="0" range_max="0" resolution="1" update_interval="3ms" data_interval="1ms"/>
		</code>
	</MWElement>	
</MWElements>
